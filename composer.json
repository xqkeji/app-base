{
    "name": "xqkeji/app-base",
    "description": "一个基于新齐低代码开发框架后台管理的基础管理模块，是整个后台管理系统的核心模块。",
    "version": "1.0.0",
    "type": "library",
    "license": "SSPL-1.0",
    "autoload": {
        "psr-4": {
            "xqkeji\\app\\base\\": "src/"
        }
    },
    "authors": [
        {
            "name": "xqkeji.cn"
        }
    ],
    "require": {
        "twbs/bootstrap": "^5.2"
    },
    "scripts": {
        "post-update-cmd": [
            "xqkeji\\app\\base\\composer\\Cmd::removeDir('www/assets/bootstrap/')",
            "xqkeji\\app\\base\\composer\\Cmd::copyDir('vendor/twbs/dist/','www/assets/bootstrap/')"
        ],
        "post-install-cmd": [
            "xqkeji\\app\\base\\composer\\Cmd::removeDir('www/assets/bootstrap/')",
            "xqkeji\\app\\base\\composer\\Cmd::copyDir('vendor/twbs/dist/','www/assets/bootstrap/')"
        ]
    }
}
